{    
    "parameters": {
        "urls": [
            {"pattern": "/pattern/.*-(\\d+)", "names": ["storyId"]}
        ],
        "servers": {
            "local": "http://localhost:8787"
        }
    },
    "backend": [
        {
            "fn": "selectGoogle",
            "target":"http://www.google.com",
            "host":"google.com"
        },
        {
            "pattern": "/different.*",
            "timeout": "1000",
            "target":"http://localhost:8787/example/backend-diff",
            "host":"localhost",
            "ttl":"10s",
            "quietFailure": true
        },
        {
            "pattern": "/quiet/.*",
            "timeout": "1000",
            "target":"http://localhost:8787/example/backend",
            "host":"localhost",
            "ttl":"10s",
            "quietFailure": true
        },
        {
            "pattern": ".*",
            "timeout": "1000",
            "target":"http://localhost:8787/example/backend",
            "host":"localhost",
            "ttl":"10s",
            "cacheKey":"backend:${url:pathname}",
            "dontPassUrl": false,
            "quietFailure": false
        }
    ],
    "statusCodeHandlers":{
        "403":{
            "fn":"handle403",
            "data":{
                "redirect":"http://www.google.com"
            }
        }
    },
    "cache": {
        "defaultNoCacheHeaders": {
            "cache-control": "private, no-cache, max-age=0, must-revalidate, no-store"
        },
        "engine": "memorycache",
        "apiEnabled": true
    }
}